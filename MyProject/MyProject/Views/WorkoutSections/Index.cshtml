@model IEnumerable<MyProject.Models.WorkoutSection>

@{
    ViewData["Title"] = "Workout Test";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.WorkoutSectionName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.WorkoutSectionName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.WorkoutSectionId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.WorkoutSectionId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.WorkoutSectionId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
